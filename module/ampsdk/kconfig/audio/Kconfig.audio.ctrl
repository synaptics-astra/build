menuconfig AMP_IP_AUDIO_MS12_ARM_V2_4
	bool "MS12 v2.4"
	default y

if AMP_IP_AUDIO_MS12_ARM_V2_4
config AMP_IP_AUDIO_MS12_ARM_V2_4_Y_LITE
