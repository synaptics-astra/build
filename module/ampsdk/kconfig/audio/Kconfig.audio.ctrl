	bool "MS12 v1.3"
	default n

if AMP_IP_AUDIO_MS12_HIFI_V1_3
config AMP_IP_AUDIO_MS12_HIFI_V1_3_B
	bool "MS12 v1.3 config B"
	default n
config AMP_IP_AUDIO_MS12_HIFI_V1_3_D
	bool "MS12 v1.3 config D"
	default n
endif

menuconfig AMP_IP_AUDIO_MS12_HIFI_V2_6
	bool "MS12 v2.6"
	default y

if AMP_IP_AUDIO_MS12_HIFI_V2_6
config AMP_IP_AUDIO_MS12_HIFI_V2_6_X
